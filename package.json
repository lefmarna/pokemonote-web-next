{
  "name": "pokemonote-web-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "fix": "prettier -w --loglevel warn '**/*.{js,jsx,ts,tsx,html,json,md}'",
    "lint": "next lint",
    "lint:fix": "npm run -s lint:es:fix && npm run -s lint:style:fix",
    "lint:es": "eslint --ignore-path .gitignore './src/**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "eslint --fix --ignore-path .gitignore './src/**/*.{js,jsx,ts,tsx}'",
    "lint:style": "stylelint --allow-empty-input './src/**/*.{css,scss}'",
    "lint:style:fix": "stylelint --fix --allow-empty-input './src/**/*.{css,scss}'",
    "lint:ts": "tsc --pretty --noEmit",
    "generate-openapi": "openapi-typescript http://localhost/openapi -o ./src/types/openapi/autoGenerated.d.ts && prettier -w ./src/types/openapi/autoGenerated.d.ts",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint --edit $1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier -w"
    ],
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier -w"
    ],
    "src/**/*.{css,scss}": [
      "stylelint --fix",
      "prettier -w"
    ],
    "*.{html,json,md}": [
      "prettier -w"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.11",
    "@mui/lab": "^5.0.0-alpha.146",
    "@mui/material": "^5.14.11",
    "@mui/x-data-grid": "^6.16.0",
    "axios": "^1.5.1",
    "next": "13.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "^0.7.7",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-interactions": "^7.4.5",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/blocks": "^7.4.5",
    "@storybook/nextjs": "^7.4.5",
    "@storybook/react": "^7.4.5",
    "@storybook/testing-library": "^0.2.1",
    "@types/node": "20.8.0",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-storybook": "^0.6.14",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "openapi-typescript": "^6.7.0",
    "prettier": "^3.0.3",
    "sass": "^1.68.0",
    "storybook": "^7.4.5",
    "stylelint": "^15.10.3",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.0.2",
    "typescript": "5.2.2"
  },
  "volta": {
    "node": "18.18.0",
    "npm": "9.8.1"
  }
}
