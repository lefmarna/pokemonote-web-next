{
  "name": "pokemonote-web-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "fix": "prettier -w --ignore-path .prettierignore --loglevel warn '**/*.{js,jsx,ts,tsx,css,scss,md}'",
    "lint": "next lint",
    "lint:fix": "npm run -s lint:es:fix && npm run -s lint:style:fix",
    "lint:es": "eslint --ignore-path .gitignore './src/**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "eslint --fix --ignore-path .gitignore './src/**/*.{js,jsx,ts,tsx}'",
    "lint:style": "stylelint --allow-empty-input './src/**/*.{css,scss}'",
    "lint:style:fix": "stylelint --fix --allow-empty-input './src/**/*.{css,scss}'",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint --edit $1"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": "eslint --fix --ignore-path .gitignore",
    "src/**/*.{css,scss}": "stylelint --fix --allow-empty-input",
    "*.{css,scss,js,jsx,ts,tsx,md}": "prettier -w  --ignore-path .prettierignore --loglevel warn"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.1",
    "@mui/x-data-grid": "^6.2.0",
    "axios": "^1.3.6",
    "next": "^13.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "^0.7.7",
    "swr": "^2.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/node": "^18.14.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-next": "^13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.1",
    "next-intercept-stdout": "^1.0.1",
    "prettier": "^2.8.7",
    "sass": "^1.62.0",
    "stylelint": "^15.5.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^3.0.0",
    "typescript": "^4.9.5"
  }
}
