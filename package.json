{
  "name": "pokemonote-web-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "fix": "prettier -w --loglevel warn '**/*.{js,jsx,ts,tsx,html,json,md}'",
    "lint": "next lint",
    "lint:fix": "npm run -s lint:es:fix && npm run -s lint:style:fix",
    "lint:es": "eslint --ignore-path .gitignore './src/**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "eslint --fix --ignore-path .gitignore './src/**/*.{js,jsx,ts,tsx}'",
    "lint:style": "stylelint --allow-empty-input './src/**/*.{css,scss}'",
    "lint:style:fix": "stylelint --fix --allow-empty-input './src/**/*.{css,scss}'",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint --edit $1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier -w"
    ],
    "src/**/*.{css,scss}": [
      "stylelint --fix",
      "prettier -w"
    ],
    "*.{html,json,md}": [
      "prettier -w"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.3",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.134",
    "@mui/material": "^5.13.5",
    "@mui/x-data-grid": "^6.8.0",
    "axios": "^1.4.0",
    "next": "13.4.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "^0.7.7",
    "swr": "^2.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@storybook/addon-essentials": "^7.0.21",
    "@storybook/addon-interactions": "^7.0.21",
    "@storybook/addon-links": "^7.0.21",
    "@storybook/blocks": "^7.0.21",
    "@storybook/nextjs": "^7.0.21",
    "@storybook/react": "^7.0.21",
    "@storybook/testing-library": "^0.1.0",
    "@types/node": "20.3.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "next-intercept-stdout": "^1.0.1",
    "prettier": "^2.8.8",
    "sass": "^1.63.4",
    "storybook": "^7.0.21",
    "stylelint": "^15.7.0",
    "stylelint-config-recess-order": "^4.2.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^3.0.0",
    "typescript": "5.1.5"
  },
  "volta": {
    "node": "18.16.0",
    "npm": "9.5.1"
  }
}
