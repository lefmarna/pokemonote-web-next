{
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-recess-order",
    "stylelint-prettier/recommended"
  ],
  "plugins": ["stylelint-order", "stylelint-prettier"],
  "rules": {
    "property-no-vendor-prefix": [
      true,
      {
        "ignoreProperties": ["appearance", "text-size-adjust"]
      }
    ]
  }
}
